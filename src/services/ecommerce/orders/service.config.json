{
  "name": "orders",
  "version": "1.0.0",
  "port": 3002,
  "domain": "ecommerce",
  "description": "Order management microservice with user and payment integration",
  "dependencies": ["users", "payments"],
  "healthCheck": "/health",
  "database": {
    "isolation": "shared",
    "migrations": true
  },
  "auth": {
    "strategy": "jwt"
  },
  "tracing": {
    "enabled": true,
    "samplingRate": 0.5
  }
}
